#include<cstring>
#include<utility>
#include <climits>
#include<unordered_map>
using namespace std;
#define printf printf_s
#define sprintf sprintf_s
#define sscanf sscanf_s
#define scanf scanf_s

int p[101];
int node[303] = {};
void build(int L,int R,int i){
	if (L == R){
		node[i] = p[L];
		return;
	}
	int M = (L + R) >> 1;
	build(L, M, i<<1);
	build(M + 1, R, (i<<1)+1);
	node[i] = node[i << 1] + node[(i << 1) + 1];
}
int x, n;//更新位置,數值大小
void update(int L, int R, int i){
	if (L == R){
		node[i] = n;
		return;
	}
	int M = (L + R) >> 1;
	build(L, M, i << 1);
	build(M + 1, R, (i << 1) + 1);
	node[i] = node[i << 1] + node[(i << 1) + 1];
}
int main(){
	int n;
	cin >> n;
	for (int i = 0; i < n; i++)
		cin >> p[i];
	build(0,n-1,1);


}
