#include <climits>
#include<unordered_map>
using namespace std;


int a[500001] = {}, buf[500001] = {};
long long msort(int left, int right){
	if (right - left <= 1)return 0;
	long long ans = 0;
	int mid = (left + right) / 2;
	ans = msort(left, mid) + msort(mid, right);
	int i=left, j=mid;
	for (int k = left; k < right; k++){
		if (i >= mid)
			buf[k] = a[j++];
		else if (j >= right)
			buf[k] = a[i++];
		else{
			if (a[i]>a[j]){
				buf[k] = a[j++];
				ans += mid - i;
			}
			else
				buf[k] = a[i++];
		}
		//k++;
	}
	for (int k = left; k < right; k++)
		a[k] = buf[k];
	return ans;
}

int main(){
	int n;
	while (~scanf("%d", &n)&&n){
		for (int i = 0; i < n; i++)
			scanf("%d", &a[i]);
		printf("%lld\n", msort(0,n));
	}
}

sample input
5
9 1 0 5 4
output
6   //輸出交換次數 使其以小到大排列 (0 1 4 5 9)

