#include<iostream>
#include<cmath>
#include<algorithm>
#include<cstring>
#include<cstdio>
#include<queue>
#include<string>
#include<map>
#include<set>
#include<string>
#include<sstream>
#include<vector>
#include<functional>
#include<unordered_map>

using namespace std;

int main() {
	FILE *t;
	//freopen_s(&t, "加密.txt", "r", stdin);
	freopen_s(&t, "解密.txt", "w", stdout);
	string s;
	while (getline(cin, s)) {
		for (int i = 0; i < s.size(); i++) {
			if ('0' <= (s[i]) && s[i] <= '9') {
				cout << char('9' - s[i] + '0');
			}
			else if (s[i] > 0 && isalpha(s[i])) {
				cout << char((int)s[i] - 1);
			}
			else
				cout << s[i];
		}
		puts("");
	}
	//fclose(stdin);
	fclose(stdout);
}
